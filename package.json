{
  "private": true,
  "scripts": {
    "dev": "umi dev",
    "build-test": "UMI_ENV=test umi build",
    "build-prod": "UMI_ENV=prod umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@umijs/preset-react": "1.x",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rebass": "^4.0.7",
    "styled-components": "^5.2.1",
    "umi": "^3.2.28"
  }
}
